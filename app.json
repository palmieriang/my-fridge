{
  "expo": {
    "name": "myfridge",
    "description": "How many times do you open the fridge and find something expired? This app is the best way to track your groceries.",
    "slug": "myfridge",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "6.0.5",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.myfridge.App",
      "versionCode": 18,
      "permissions": []
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*",
      "assets/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.myfridge.App"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35
          }
        }
      ],
      "expo-font",
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "fe9ca302-54f0-46b9-bfc8-123e258763e0"
      }
    }
  }
}
