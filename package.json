{
  "main": "index.js",
  "scripts": {
    "start": "npx expo start",
    "start:clear": "npx expo start --clear",
    "android": "npx expo start --android",
    "ios": "npx expo start --ios",
    "web": "npx expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "lint": "eslint ./src",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "typecheck": "tsc --noEmit",
    "clean": "npx expo install --fix",
    "clean:cache": "npx expo start --clear && yarn cache clean",
    "validate": "yarn typecheck && yarn lint && yarn format:check",
    "functions:build": "cd functions && yarn build",
    "functions:deploy": "cd functions && yarn deploy",
    "functions:serve": "cd functions && yarn serve",
    "emulators:start": "firebase emulators:start",
    "emulators:ui": "firebase emulators:start --only firestore,storage,auth,functions,ui",
    "postinstall": "node ./eas-build-pre-install.js"
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-firebase/app": "^22.2.1",
    "@react-native-firebase/auth": "^22.2.1",
    "@react-native-firebase/firestore": "^22.2.1",
    "@react-native-firebase/messaging": "^22.4.0",
    "@react-native-firebase/storage": "^22.2.1",
    "@react-native-google-signin/google-signin": "^15.0.0",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.3.29",
    "expo": "~54.0.25",
    "expo-build-properties": "~1.0.9",
    "expo-constants": "~18.0.10",
    "expo-font": "~14.0.9",
    "expo-image-picker": "~17.0.8",
    "expo-localization": "~17.0.7",
    "expo-status-bar": "~3.0.8",
    "i18n-js": "^4.4.3",
    "lottie-react-native": "~7.3.1",
    "moment": "^2.30.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-dotenv": "^2.4.3",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-modal-datetime-picker": "^17.1.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1",
    "react-refresh": "^0.14.2"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/plugin-transform-optional-chaining": "^7.27.1",
    "@types/react": "~19.1.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "~54.0.0",
    "eslint": "^8.56.0",
    "eslint-config-universe": "^15.0.3",
    "prettier": "^3.5.3",
    "typescript": "~5.9.2"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "firebase",
          "moment"
        ]
      }
    }
  }
}
